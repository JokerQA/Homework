							"});\r",
							"\r",
							"pm.test(\"Check salary\", function () {\r",
							"    pm.expect(jsonData.salary).to.eql(salary_r_request);\r",
							"});\r",
							"\r",
							"let reqBodyName = pm.request.body.formdata.get(\"name\"); // Забираем из реквеста значение name\r",
							"let reqBodyAge = pm.request.body.formdata.get(\"age\"); // Забираем из реквеста значение age\r",
							"let reqBodySalary = parseInt(pm.request.body.formdata.get(\"salary\")); // Забираем из реквеста значение salary\r",
							"\r",
							"// Сравниваем поле name из ответа и запроса\r",
							"pm.test(\"Response name = Request name\", function () {\r",
							"    pm.expect(jsonData.name).to.eql(reqBodyName);\r",
							"});\r",
							"\r",
							"// Сравниваем поле age из ответа и запроса\r",
							"pm.test(\"Response age = Request age\", function () {\r",
							"    pm.expect(jsonData.age).to.eql(reqBodyAge);\r",
							"});\r",
							"\r",
							"// Сравниваем поле salary из ответа и запроса\r",
							"pm.test(\"Response salary = Request salary\", function () {\r",
							"    pm.expect(jsonData.salary).to.eql(reqBodySalary);\r",
							"});\r",
